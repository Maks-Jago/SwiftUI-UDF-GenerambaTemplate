import SwiftUI

struct ConfirmPasswordValidator: BlankValidation, Validation {
    
    var password: String
    
    func validate(_ value: String) -> Bool {
        let passwordValidator = PasswordValidator()
                
        if blankValidation(string: password) {
            return false
        }
        
        if blankValidation(string: password) {
            return false
        }
        
        guard value == password else {
            return false
        }

        if passwordValidator.validate(password) {
            return false
        }
        
        if passwordValidator.validate(value) {
            return false
        }
        
        return true
    }
}
