import Foundation
import SwiftUI_UDF

enum {{ module_info.name }}Flow: IdentifiableFlow {
    case none, loading
    
    init() { self = .none }
    
    mutating func reduce(_ action: AnyAction) {
        switch action.value {
            
        case let action as Actions.Error where action.id == Self.id:
            self = .none
            
        default:
            break
        }
    }
}